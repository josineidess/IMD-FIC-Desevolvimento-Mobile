var a = vetor[100]
var b = 0
var c = 0
para i de 99 até 0 passo -1 faça
    a[i] = leia_inteiro
fim

para i de 0 até 98 faça
    b = i
    para j de i+1 até 100-1 faça
        se a[j] < a[b] então
            b = j
        fim
    fim
    se a[i] <> a[b] então
        c = a[i]
        a[i] = a[b]
        a[b] = c
     fim
fim

para i de 0 até 99 faça
    escreva a[i]
fim




