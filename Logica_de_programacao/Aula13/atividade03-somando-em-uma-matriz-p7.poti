var m := matriz[5,10]

para linha de 0 até 4 faça
    para coluna de 0 até 9 faça
        m[linha][coluna] = leia_inteiro
    fim
fim

var n := leia_inteiro
var soma := 0

para linha de 0 até 4 faça
    soma = soma + m[linha][n-1]
fim

para linha de 0 até 4 faça
    para coluna de 0 até 9 faça
        se coluna <> 9 então
           imprima "{m[linha][coluna]} "
        senão
           imprima "{m[linha][coluna]}"
        fim
    fim
    escreva
fim

escreva

escreva soma





