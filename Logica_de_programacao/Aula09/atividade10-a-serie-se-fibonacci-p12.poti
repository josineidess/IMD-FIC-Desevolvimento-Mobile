var n := leia_inteiro

se n < 46 então
   var x1 := 0
   var x2 := 1 
   var x3 := x1 + x2

   se n > 1 então
      escreva x1
      escreva x2
   senão
      se n == 1 então
          escreva x1
      fim
   fim
   
   para i de 2 até n-1 faça
       escreva x3
       x1 = x2
       x2 = x3
       x3 = x1 + x2
   fim
fim
